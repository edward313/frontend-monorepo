{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "ES2017",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"isolatedModules": true,
		"skipLibCheck": true
	},
	"exclude": ["node_modules"]
}
